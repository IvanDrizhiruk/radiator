entity BuildState {
	instancesName String,
	state String,
	errorMessage String,	
	lastRunTimestemp Long,
    extractingDate ZonedDateTime
}

entity Commiter {
	name String,
	email String
}

entity IntegrationTestResult {
	total Integer,
	passed Integer,
	failed integer,
	extractingDate ZonedDateTime
}

relationship ManyToMany {
  BuildState{commiter(name)} to Commiter{buildState}
}

